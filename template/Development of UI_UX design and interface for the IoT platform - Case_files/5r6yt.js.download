validateInteger(event, 'date');
});
function is_valid_sms(sms){
sms=sms.replace(/\b(0(?!\b))+/g, "");
var tempSms=sms.replace(/(|\(|\)|\.|\-)/g, '');
if(tempSms.length > 19||tempSms.length < 6||tempSms.charAt(0)!='+'){
return false;
}
return true;
}
